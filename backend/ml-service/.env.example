# AquaHarvest ML Service Environment Variables

# Database Configuration
DATABASE_URL=postgresql://aquaharvest:password@localhost:5432/aquaharvest_db
DB_HOST=localhost
DB_PORT=5432
DB_NAME=aquaharvest_db
DB_USER=aquaharvest
DB_PASSWORD=password

# Service Configuration
ML_SERVICE_PORT=8000
ML_SERVICE_HOST=0.0.0.0
LOG_LEVEL=INFO

# External API Keys (for production weather data)
OPENWEATHER_API_KEY=your_openweather_api_key_here
GOOGLE_EARTH_ENGINE_KEY=path/to/gee_service_account.json

# Model Configuration
MODEL_CACHE_SIZE=100
PREDICTION_CACHE_TTL=3600
BATCH_SIZE=32

# Redis Configuration (optional, for caching)
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Security
SECRET_KEY=your-secret-key-here
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# Google Cloud Storage (for model persistence)
GCS_BUCKET_NAME=aquaharvest-models
GCS_PROJECT_ID=your-project-id

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Development/Debug
DEBUG=false
RELOAD=true